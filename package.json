{
  "name": "devcontainer-enforcer",
  "displayName": "DevContainer Enforcer",
  "description": "Alert-only guard for Dev Containers with ancestor .devcontainer lookup.",
  "version": "0.1.0",
  "publisher": "yourPublisherId",
  "engines": {
    "vscode": "^1.90.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "categories": [
    "Other"
  ],
  "private": true,
  "license": "SEE LICENSE IN LICENSE.txt",
  "keywords": [
    "devcontainer",
    "development",
    "container",
    "enforcer",
    "vscode"
  ],
  "scripts": {
    "vscode:prepublish": "npm run esbuild",
    "typecheck": "tsc -p . --noEmit",
    "esbuild": "npm run typecheck && node esbuild.config.mjs",
    "dev": "tsc -w -p . --noEmit false --outDir out --sourceMap true",
    "package": "bash ./production-build.sh"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/vscode": "^1.90.0",
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.10",
    "javascript-obfuscator": "^4.1.0",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ultra-mega-apps/devcontainer-enforcer-free.git"
  }
}
